  "scripts": {
    "sync": "ts-node utils/sync.ts",
    "start:elm": "elm-pages develop",
    "build:elm": "elm-pages build",
    "start:serve": "COCKPIT_MODE=start ts-node utils/serve.ts",
    "preview:copy": "cp -R ___preview dist",
    "preview:live": "yarn preview:copy && elm-live src/Preview.elm --port=3000 --dir=dist --start-page=dist/___preview/index.html -- --output=dist/preview.js",
    "start:preview": "npm-run-all --parallel preview:live start:serve",
    "start": "elm-pages dev",
    "build": "elm-pages build",
    "serve": "COCKPIT_MODE=serve ts-node utils/serve.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "cockpit-http-client": "1.0.1",
    "elm-pages": "2.1.4",
    "environment-decoder": "1.1.0"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/parseurl": "1.3.1",
    "elm": "0.19.1-5",
    "elm-format": "0.8.5",
    "express": "4.17.1",
    "got": "11.8.2",
    "http-proxy-middleware": "1.3.0",
    "npm-run-all": "4.1.5",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
